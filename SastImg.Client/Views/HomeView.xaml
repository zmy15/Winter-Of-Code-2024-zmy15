<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="SastImg.Client.Views.HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:SastImg.Client.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" d:DataContext="{d:DesignInstance Type=local:HomeViewModel}"
    Background="Transparent"
    mc:Ignorable="d">

    <Grid>
        <StackPanel>
            <TextBlock
                Margin="56,40"
                Style="{ThemeResource TitleLargeTextBlockStyle}"
                Text="SAST Image" />
            <Button Content="112"
                    Style="{ThemeResource EllipsisButton}"
                    Command="{Binding GetImage}"/>
        </StackPanel>
    </Grid>
</Page>
